    "babel-preset-stage-0": "^6.0.15",
    "bootstrap": "^4.0.0-alpha.2",
    "css-loader": "^0.23.1",
    "express": "^4.13.4",
    "express-http-proxy": "^0.7.0",
    "file-loader": "^0.9.0",
    "jquery": "^2.2.4",
    "json-loader": "^0.5.4",
    "lodash": "^4.13.1",
    "memjs": "^0.10.0",
    "node-sass": "^3.8.0",
    "postcss-loader": "^0.9.1",
    "react": "^0.14.6",
    "react-dom": "^0.14.6",
    "sass-loader": "^4.0.0",
    "style-loader": "^0.13.1",
    "tether": "^1.3.2",
    "webpack": "^1.13.1"
